digraph ControlFlowGraph {
    node [shape=rectangle];
    entry;
    longestSt;
    phi;
    icmp;
    br;
    invoke;
    landingpad;
    tail;
    resume;
    add;
    ptrtoint;
    sub;
    ashr;
    ret;
    exit;

    entry -> longestSt;
    longestSt -> phi;
    phi -> icmp;
    icmp -> br;
    br -> invoke;
    invoke -> landingpad;
    landingpad -> tail;
    tail -> resume;
    resume -> add;
    add -> ptrtoint;
    ptrtoint -> sub;
    sub -> ashr;
    ashr -> ret;
    br -> ret;
    br -> exit;
}